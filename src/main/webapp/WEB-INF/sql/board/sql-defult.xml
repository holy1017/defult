<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="defult">

	<select
		id="userList"
		parameterType="board.UserVO"
		resultType="board.UserVO">
		select u_no,
		u_id,
		u_pw,
		u_nick,
		u_indate
		from defult_user
		order
		by u_no desc
	</select>
	<select
		id="boardList"
		parameterType="board.BoardVO"
		resultType="board.BoardVO">
		select b_no,
		b_title,
		b_content,
		b_indate,
		b_eddate,
		u_no
		from
		defult_board
		order by b_no desc
	</select>
	<select
		id="replyList"
		parameterType="board.ReplyVO"
		resultType="board.ReplyVO">
		select r_no,
		r_content,
		u_no,
		b_no,
		r_indate,
		r_eddate
		from
		defult_reply
		order by r_no desc
	</select>
	<insert
		id="userInsert"
		parameterType="board.UserVO">
		
		</insert>
	<insert
		id="boardInsert"
		parameterType="board.BoardVO">
		
		</insert>
	<insert
		id="replyInsert"
		parameterType="board.ReplyVO">
		
		</insert>

